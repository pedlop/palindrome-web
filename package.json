{
  "name": "palindrome-web",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "build": "npm run build:ssr",
    "start": "npm run serve:ssr",
    "start:back": "webpack --config webpack.server.config.local.js --watch",
    "start:front": "ng serve --proxy-config proxy.conf.json",
    "start:app": "concurrently --kill-others \"npm run start:back\" \"npm run start:front\"",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "compile:server:local": "webpack --config webpack.server.config.local.js --progress --colors",
    "build:local": "npm run build:client-and-server-bundles && npm run compile:server:local",
    "compile:server": "webpack --config webpack.server.config.production.js --progress --colors",
    "build:client-and-server-bundles": "ng build --prod && ng run palindrome-web:server:production",
    "serve:ssr": "node dist/server",
    "build:ssr": "npm run build:client-and-server-bundles && npm run compile:server"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.2.15",
    "@angular/cdk": "^7.3.7",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/http": "~7.2.0",
    "@angular/material": "^7.3.7",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/platform-server": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@nguniversal/express-engine": "^7.1.1",
    "@nguniversal/module-map-ngfactory-loader": "0.0.0",
    "body-parser": "^1.19.0",
    "core-js": "^2.5.4",
    "express": "^4.15.2",
    "express-http-proxy": "^1.5.1",
    "ngx-seo-page": "^1.0.3",
    "rxjs": "~6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.13.0",
    "@angular/cli": "~7.3.8",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@types/express": "^4.16.1",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "codelyzer": "~4.5.0",
    "concurrently": "^4.1.0",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~4.0.0",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "nodemon-webpack-plugin": "^4.0.8",
    "protractor": "~5.4.0",
    "regexp-replace-loader": "^1.0.1",
    "tmp": "^0.1.0",
    "ts-loader": "^5.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.2.2",
    "webpack-cli": "^3.1.0",
    "webpack-merge": "^4.2.1"
  }
}
